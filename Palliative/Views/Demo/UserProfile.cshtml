                    <a role="button" data-toggle="collapse" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                        Child Information
                    </a>
                </h4>
            </div>
            <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                <div class="panel-body">
                    <div class="left">
                        Name:<br>
                        <p class="sample">John Doe</p>
                        Gender:<br>
                        <p class="sample">Male</p>
                    </div>
                    <div class="right">
                        Address:<br>
                        <p class="sample">123 Maple St.</p>
                        Birthday:<br>
                        <p class="sample">April 1st, 2010</p>
                    </div>
                </div>
            </div>
        </div>


